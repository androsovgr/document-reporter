<?xml version="1.0" encoding="UTF-8" ?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="template.xsd">
	<attribute key="tittle" label="Название реферата"
		retrieverClass="ru.mephi.dr.parser.retriever.CellValueRetriever">
		<parameter>
			<key>expression</key>
			<value>(.*)</value>
		</parameter>
		<parameter>
			<key>table-position</key>
			<value>first</value>
		</parameter>
		<parameter>
			<key>row-number</key>
			<value>2</value>
		</parameter>
		<parameter>
			<key>column-number</key>
			<value>1</value>
		</parameter>
	</attribute>
	<attribute key="subject" label="Предмет"
		retrieverClass="ru.mephi.dr.parser.retriever.CellValueRetriever">
		<parameter>
			<key>expression</key>
			<value>Реферат по (.*) аспиранта кафедры.*</value>
		</parameter>
		<parameter>
			<key>table-position</key>
			<value>first</value>
		</parameter>
		<parameter>
			<key>row-number</key>
			<value>3</value>
		</parameter>
		<parameter>
			<key>column-number</key>
			<value>1</value>
		</parameter>
		<parameter>
			<key>line</key>
			<value>2</value>
		</parameter>
	</attribute>
	<attribute key="department" label="Кафедра"
		retrieverClass="ru.mephi.dr.parser.retriever.CellValueRetriever">
		<parameter>
			<key>expression</key>
			<value>.*кафедры №?(.*)</value>
		</parameter>
		<parameter>
			<key>table-position</key>
			<value>first</value>
		</parameter>
		<parameter>
			<key>row-number</key>
			<value>3</value>
		</parameter>
		<parameter>
			<key>column-number</key>
			<value>1</value>
		</parameter>
		<parameter>
			<key>line</key>
			<value>2</value>
		</parameter>
	</attribute>
	<attribute key="name" label="Имя аспиранта"
		retrieverClass="ru.mephi.dr.parser.retriever.CellValueRetriever">
		<parameter>
			<key>expression</key>
			<value>(.*)</value>
		</parameter>
		<parameter>
			<key>table-position</key>
			<value>first</value>
		</parameter>
		<parameter>
			<key>row-number</key>
			<value>3</value>
		</parameter>
		<parameter>
			<key>column-number</key>
			<value>1</value>
		</parameter>
		<parameter>
			<key>line</key>
			<value>3</value>
		</parameter>
	</attribute>
	<attribute key="specialty" label="Специальность аспиранта"
		retrieverClass="ru.mephi.dr.parser.retriever.CellValueRetriever">
		<parameter>
			<key>expression</key>
			<value>(С|с).*(\d\d\.\d\d\.\d\d)</value>
		</parameter>
		<parameter>
			<key>expression-group-number</key>
			<value>2</value>
		</parameter>
		<parameter>
			<key>table-position</key>
			<value>first</value>
		</parameter>
		<parameter>
			<key>row-number</key>
			<value>3</value>
		</parameter>
		<parameter>
			<key>column-number</key>
			<value>1</value>
		</parameter>
		<parameter>
			<key>line</key>
			<value>4</value>
		</parameter>
	</attribute>
	<attribute key="pageCount" label="Количество страниц"
		retrieverClass="ru.mephi.dr.parser.retriever.PageCountRetriever" />
</template>