<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane style="-fx-padding: 5px;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.mephi.dr.ui.action.view.WorkController">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
      <ColumnConstraints maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
   </columnConstraints>
   <children>
      <Button mnemonicParsing="false" onAction="#receive" prefHeight="31.0" prefWidth="200.0" text="Получить почту" GridPane.columnIndex="1" />
      <Button mnemonicParsing="false" onAction="#report" prefHeight="31.0" prefWidth="200.0" text="Составить отчет" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <ComboBox fx:id="reportTypesBox" prefWidth="150.0" GridPane.rowIndex="1" />
   </children>
</GridPane>
